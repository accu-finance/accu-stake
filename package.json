{
  "name": "@accu-finance/accu-stake",
  "version": "1.0.0",
  "description": "Safety Module for the Aave Protocol",
  "files": [
    "contracts",
    "artifacts"
  ],
  "scripts": {
    "prepare": "node ./.setup.js && npm run compile",
    "lint": "eslint \"**/*.{js,ts}\" && solhint contracts/**/*.sol",
    "lint:fix": "eslint --fix \"**/*.{js,ts}\" && solhint --fix contracts/**/*.sol",
    "format": "npx prettier --check \"**/*.{ts,js,sol}\"",
    "format:fix": "npx prettier --write \"**/*.{ts,js,sol}\"",
    "compile": "SKIP_LOAD=true hardhat compile",
    "compile:force": "npm run compile -- --force",
    "test": "npx hardhat test",
    "coverage": "npx hardhat coverage --network hardhat",
    "clean": "rm -rf typechain/ cache/ artifacts/",
    "prepublishOnly": "npm run compile"
  },
  "devDependencies": {
    "@ethersproject/abstract-signer": "^5.4.1",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.10",
    "@nomiclabs/hardhat-solhint": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "3.0.1",
    "@tenderly/hardhat-tenderly": "^1.0.11",
    "@typechain/ethers-v5": "^6.0.1",
    "@types/chai": "^4.2.15",
    "@types/lowdb": "^1.0.9",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "chai": "^4.3.0",
    "chai-ethers": "0.0.1",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eth-sig-util": "^3.0.1",
    "ethereum-waffle": "^3.3.0",
    "ethereumjs-util": "^7.0.2",
    "ethers": "^5.4.6",
    "hardhat": "^2.3.0",
    "hardhat-deploy": "^0.9.0",
    "hardhat-deploy-ethers": "^0.3.0-beta.10",
    "hardhat-gas-reporter": "^1.0.4",
    "hardhat-typechain": "^0.3.5",
    "lowdb": "1.0.0",
    "prettier": "^2.4.1",
    "prettier-plugin-solidity": "^1.0.0-beta.17",
    "pretty-quick": "^3.1.1",
    "solhint": "^3.3.2",
    "solhint-plugin-prettier": "0.0.5",
    "solidity-coverage": "^0.7.17",
    "ts-generator": "^0.1.1",
    "ts-node": "^9.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typechain": "^4.0.2",
    "typescript": "^4.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern 'contracts/**/*.sol' --pattern '**/*.{ts,js}'"
    }
  },
  "author": "Aave",
  "contributors": [
    "Ernesto Boado <ernesto@aave.com>",
    "Emilio Frangella <emilio@aave.com>",
    "Andrey Kozlov <andrey@aave.com>",
    "David Racero <david.k@aave.com>"
  ],
  "license": "AGPL-3.0",
  "dependencies": {},
  "keywords": [
    "aave",
    "stake",
    "token",
    "safety",
    "module",
    "ethereum"
  ]
}
